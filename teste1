-- Voltar
backBtn.MouseButton1Click:Connect(function()
    if not savedPos then
        status2.Text = "âš ï¸ NENHUMA POSIÃ‡ÃƒO SALVA!\nâŒ Use o botÃ£o SALVAR POSIÃ‡ÃƒO primeiro"
        status2.TextColor3 = Color3.fromRGB(255, 200, 100)
        return
    end
    
