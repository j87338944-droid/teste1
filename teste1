-- Fun√ß√µes
local function createServerButton(serverData)
    local Button = Instance.new("TextButton")
    Button.Size = UDim2.new(1, -10, 0, 60)
    Button.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
    Button.BorderSizePixel = 0
    Button.Font = Enum.Font.Gotham
    Button.TextColor3 = Color3.fromRGB(255, 255, 255)
    Button.TextSize = 14
    Button.TextXAlignment = Enum.TextXAlignment.Left
    Button.TextYAlignment = Enum.TextYAlignment.Top
    
    local brainrotText = serverData.brainrot or "Desconhecido"
    local pingText = serverData.ping or "?"
    local playersText = serverData.playing .. "/" .. serverData.maxPlayers
    
    Button.Text = string.format("  üß† Brainrot: %s\n  üë• Players: %s | üì∂ Ping: %sms", 
        brainrotText, playersText, pingText)
    
    local ButtonCorner = Instance.new("UICorner")
    ButtonCorner.CornerRadius = UDim.new(0, 6)
    ButtonCorner.Parent = Button
    
    Button.MouseButton1Click:Connect(function()
        Button.Text = "  ‚è≥ Entrando no servidor..."
        Button.BackgroundColor3 = Color3.fromRGB(100, 200, 100)
        wait(1)
        TeleportService:TeleportToPlaceInstance(GAME_ID, serverData.id, LocalPlayer)
    end)
    
    return Button
end

local function searchServers()
    if isSearching then return end
    isSearching = true
    
    QuickSearchButton.Text = "‚è≥ BUSCANDO..."
    QuickSearchButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
    
    -- Limpar lista atual
    for _, child in pairs(ScrollFrame:GetChildren()) do
        if child:IsA("TextButton") then
            child:Destroy()
        end
    end
    
    -- Simular busca de servidores (substitua com API real se dispon√≠vel)
    task.spawn(function()
        local mockServers = {
            {id = "1", playing = 8, maxPlayers = 12, brainrot = "60m", ping = 45},
            {id = "2", playing = 10, maxPlayers = 12, brainrot = "50m", ping = 78},
            {id = "3", playing = 6, maxPlayers = 12, brainrot = "60m", ping = 32},
            {id = "4", playing = 11, maxPlayers = 12, brainrot = "50m", ping = 91},
            {id = "5", playing = 5, maxPlayers = 12, brainrot = "60m", ping = 55},
        }
        
        for _, serverData in ipairs(mockServers) do
            local button = createServerButton(serverData)
            button.Parent = ScrollFrame
            wait(0.1)
        end
        
        QuickSearchButton.Text = "‚úÖ " .. #mockServers .. " SERVIDORES"
        QuickSearchButton.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
        wait(2)
        QuickSearchButton.Text = "‚ö° BUSCAR R√ÅPIDO"
        QuickSearchButton.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
        isSearching = false
    end)
end

-- Eventos dos Bot√µes
QuickSearchButton.MouseButton1Click:Connect(searchServers)

SavePosButton.MouseButton1Click:Connect(function()
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        savedPosition = LocalPlayer.Character.HumanoidRootPart.CFrame
        StatusLabel.Text = "‚úÖ Posi√ß√£o salva!\n" .. tostring(savedPosition.Position)
        StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 100)
        SavePosButton.BackgroundColor3 = Color3.fromRGB(0, 255, 100)
        wait(1)
        SavePosButton.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
    else
        StatusLabel.Text = "‚ùå Erro: Personagem n√£o encontrado!"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    end
end)

TeleportBackButton.MouseButton1Click:Connect(function()
    if savedPosition then
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local hrp = LocalPlayer.Character.HumanoidRootPart
            local humanoid = LocalPlayer.Character:FindFirstChild("Humanoid")
            
            -- Desabilitar f√≠sica temporariamente
            if humanoid then
                humanoid:ChangeState(Enum.HumanoidStateType.Physics)
            end
            
            -- Teleportar usando diferentes m√©todos para maior compatibilidade
            hrp.CFrame = savedPosition
            hrp.Anchored = true
            task.wait(0.1)
            hrp.Anchored = false
            
            if humanoid then
                humanoid:ChangeState(Enum.HumanoidStateType.GettingUp)
            end
            
            StatusLabel.Text = "‚úÖ Teleportado de volta!"
            StatusLabel.TextColor3 = Color3.fromRGB(0, 255, 100)
            TeleportBackButton.BackgroundColor3 = Color3.fromRGB(0, 255, 100)
            wait(1)
            TeleportBackButton.BackgroundColor3 = Color3.fromRGB(200, 100, 0)
        else
            StatusLabel.Text = "‚ùå Erro: Personagem n√£o encontrado!"
            StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        end
    else
        StatusLabel.Text = "‚ùå Nenhuma posi√ß√£o salva ainda!"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
    end
end)

NextButton.MouseButton1Click:Connect(function()
    ServersTab.Visible = false
    PositionTab.Visible = true
    NextButton.Visible = false
    BackButton.Visible = true
end)

BackButton.MouseButton1Click:Connect(function()
    ServersTab.Visible = true
    PositionTab.Visible = false
    NextButton.Visible = true
    BackButton.Visible = false
end)

-- Iniciar busca autom√°tica ao carregar
wait(1)
searchServers()

print("‚úÖ Brainrot Finder carregado com sucesso!")
